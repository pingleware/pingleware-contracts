{
  "name": "wallet-creator",
  "version": "1.0.0",
  "description": "Am ethereum wallet creator with a primary account number association",
  "main": "index.js",
  "build": {
    "appId": "work.pingleware.walletcreator",
    "buildVersion": "1",
    "productName": "Waller Creator",
    "copyright": "Copyright Â© 2023 PRESSPAGE ENTERTAINMENT INC",
    "win": {
      "target": [
        {
          "target": "appx",
          "arch": [
            "x64",
            "ia32",
            "arm64"
          ]
        }
      ],
      "icon": "image/logo.png"
    },
    "mac": {
      "category": "public.app-category.finance",
      "icon": "image/logo.icns"
    },
    "linux": {
      "target": "snap",
      "category": "Finance",
      "icon": "image/logo.png"
    },
    "files": [],
    "extraFiles": []
  },
  "scripts": {
    "start": "node . --electron=false",
    "start:electron": "electron .",
    "help": "node . --help",
    "build:mac": "electron-builder --mac --universal",
    "build:win": "electron-builder --win --universal",
    "build:snap": "snapcraft remote-build --build-on=amd64,arm64",
    "create:icns": "./icns_creator.sh image/logo.png image/logo"
  },
  "keywords": [],
  "author": "PRESSPAGE ENTERTAINMENT INC dba PINGLEWARE <maintainers@pingleware.work>",
  "license": "CC-BY-4.0",
  "bugs": {
    "url": "https://pingleware.support"
  },
  "dependencies": {
    "bip39": "^3.1.0",
    "crypto": "^1.0.1",
    "crypto-js": "^4.2.0",
    "ethereumjs-util": "^7.1.5",
    "qrcode": "^1.5.3",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "electron": "^28.1.0",
    "electron-builder": "^24.9.1"
  }
}
